
    <h1>Add New Movie</h1>
    <div class="container">
        <form action="/memes/searchMeme" method="POST" enctype=multipart/form-data>
            <label for="memeTitle">Title</label>
            <input type="text" id="memeTitle" name="memeTitle" placeholder="The title of the meme">

            <select name='genreSelect' id="genreSelect">
              {{#each genres}}
              <option value="{{_id}}">{{title}}</option>
              {{/each}}
            </select>
            <input type="submit" value="Submit">

        </form>
    </div>
